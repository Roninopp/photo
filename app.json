    "keywords": [
        "telegram",
        "logo maker",
        "logo",
        "bot"
    ],
    "repository": "https://github.com/TechShreyash/TechZ-Logo-Maker-Bot",
    "website": "https://t.me/TechZBots",
    "success_url": "https://t.me/TechZLogoMakerBot",
    "env": {
        "API_ID": {
            "description": "Get this value from my.telegram.org"
